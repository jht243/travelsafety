{
  "name": "is-it-safe-web",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "esbuild src/main.tsx --bundle --format=esm --minify --define:process.env.NODE_ENV=\"production\" --outfile=../assets/is_it_safe.js",
    "dev": "esbuild src/main.tsx --bundle --format=esm --outfile=dist/bundle.js --watch --servedir=. --serve=8000"
  },
  "dependencies": {
    "lucide-react": "^0.554.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.11",
    "esbuild": "^0.21.5",
    "typescript": "^5.6.3"
  }
}
